* {
    margin:0;
    padding:0;
}

/*FONTS*/

@font-face {
    font-family: 'pt_monoregular';
    src: url('../fonts/ptmono/ptm55f_w.eot');
    src: url('../fonts/ptmono/ptm55f_w.eot?#iefix') format('embedded-opentype'),
    url('../fonts/ptmono/ptm55f_w.woff') format('woff'),
    url('../fonts/ptmono/ptm55f_w.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'pt_monobold';
    src: url('../fonts/ptmono/ptm75f_w.eot');
    src: url('../fonts/ptmono/ptm75f_w.eot?#iefix') format('embedded-opentype'),
    url('../fonts/ptmono/ptm75f_w.woff') format('woff'),
    url('../fonts/ptmono/ptm75f_w.ttf') format('truetype');
    font-weight: bold;
    font-style: normal;

}
/*FONTS*/

html,body {
    height:100%;
}
body {
    width: 100%;
    margin: 0;
    padding: 0;
    color: #000;
    background:#fff;
    font-size: 14px;
    line-height: 1.4;
    font-family: 'pt_monoregular';
}

.page {
    max-width: 1280px;
    min-height: 780px;
    margin: 0 auto;
}

.all {
    position: relative;
    width: 100%;
    min-height: 100%;
    height: auto !important;
    height: 100%;
    background: #fff;
}
.container {
    max-width: 990px;
}
.container.inside {
    max-width: 740px;
}
a {
    border: none;
    outline: none;
    color: #000;
}
a:hover, a:focus, a:active {
    outline: none;
    color: #000;
    text-decoration: none;
}
.left {
    float: left;
}
.right {
    float: right;
}
table {
    width: 100%;
}
input, select, textarea {
    width: 100%;
    border: 1px solid #B0E758;
    padding: 4px 10px;
    outline: none;
}
button, .button {
    display: inline-block;
    font-size: 16px;
    text-transform: uppercase;
    background: #B0E758;
    border: none;
    box-shadow: none;
    outline: none;
    letter-spacing: 1px;
    padding: 1.1rem 2.9rem;
    color: rgba(0,0,0,.88);
    /*text-align: center;
    -webkit-transition: -webkit-transform 0.15s, box-shadow 0.15s;
    transition: transform 0.15s, box-shadow 0.15s;
    -webkit-transform: rotateX(0deg) translateY(0);
    transform: rotateX(0deg) translateY(0);
    cursor: pointer;
    -webkit-font-smoothing: antialiased;*/
}
.button.info {
    font-size: 14px;
    line-height: 17px;
    padding: 0.75rem 1.6rem;
}
