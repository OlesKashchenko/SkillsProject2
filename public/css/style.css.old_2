h1 {
    font-size: 20px;
    line-height: 24px;
    text-transform: uppercase;
    margin: 0px;
    letter-spacing: -0.2px;
}
h1, h2, h3, h4, h5, h6 {
    font-weight: normal;
}
.remove {
    position: relative;
    display: inline-block;
    width: 22px;
    height: 22px;
    border:1px solid #b0e758;
    /*background: url('../images/sprite.png') no-repeat 0px 0px;*/
    cursor: pointer;
    -webkit-transition: all .3s ease 0s;
    transition: all .3s ease 0s;
}
.remove:before {
    content: '';
    position: absolute;
    top:1px;
    left: 9px;
    width: 2px;
    height: 18px;
    background: #b0e758;
    -moz-transform: rotate(45deg);  /* Firefox */
  -o-transform: rotate(45deg);  /* Opera */
  -webkit-transform: rotate(45deg);  /* Safari y Chrome */
}
.remove:after {
    content: '';
    position: absolute;
    top:1px;
    left: 9px;
    width: 2px;
    height: 18px;
    background: #b0e758;
    -moz-transform: rotate(-45deg);  /* Firefox */
  -o-transform: rotate(-45deg);  /* Opera */
  -webkit-transform: rotate(-45deg);  /* Safari y Chrome */
}

