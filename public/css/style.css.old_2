    }
    .result_item_body tr:first-child td:first-child+* {
        padding-top: 5px;
    }
    .result_item_body tr td:first-child {
        padding-top: 22px;
        border-right: none;
    }
}
@media (max-width: 420px) {
    button, .button {
        padding: 1.1rem 1.5rem;
    }
    .menu a, .float_link {
        font-size: 12px;
         padding: 10px;
    }
    .result_item_header h2 {
        font-size: 20px;
        line-height: 22px;
    }
    .result_item {
        font-size: 14px;
    }
    .result_item_body p {
        line-height: 18px;
    }
    .data_block {
        font-size: 14px;
    }
    .data_menu_container a {
        font-size: 14px;
        padding: 7px 5px 5px;
    }
    .data_body table td, .data_body table td:first-child, #user_register table, #user_register button {
        width: 100%;
    }
    .data_body input {
        max-width: 100%;
    }
    .data_body table td, .data_body .forgot_pass {
        display: block;
    }
    .data_body .forgot_pass {
        padding-left: 0px;
    }
    .data_body table tr td:first-child+* {
        padding-top: 0px;
    }
    .data_head, .data_block .soc, .data_menu {
        padding-left: 15px;
        padding-right: 15px;
    }
    .data_body {
        padding: 15px;
    }
    .data_body button {
        margin-left: 0px;
    }
}
/*media*/

